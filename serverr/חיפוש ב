להכניס למשתנה את* higet הגובה של התמונה image.hight
ולפי זה לעבור עם הלולאה
int HEIGHT =image.hight;
int WIDTH = image.width;

// get the jpg image
Bitmap bitmap;
using(Stream bmpStream = System.IO.File.Open(fileName, System.IO.FileMode.Open )){
     Image image = Image.FromStream(bmpStream);
     bitmap = new Bitmap(image);
}

for (int x = 0; x < HEIGHT; x++){
  for (int y = 0; y < WIDTH; y++){
    Color pixelColor = bitmap.GetPixel(x, y);
    // check if it's black or a shade of black
    // e.g. if it belongs to an array of colors..etc
    // if so, record the coordinates (x,y)
  }
}
